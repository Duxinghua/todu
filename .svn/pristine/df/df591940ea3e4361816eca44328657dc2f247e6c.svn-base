import request from '@/utils/request'

/** ****************岗位**********************/
/**
 * 岗位列表
 * @returns {AxiosPromise}
 */
export function posList() {
  return request({
    url: '/admin/pos/list',
    method: 'get'
  })
}

/**
 * 专业列表
 * @returns {AxiosPromise}
 */
export function majorList(params) {
  return request({
    url: '/admin/major/list',
    method: 'get',
    params
  })
}

/**
 * 角色列表
 * @returns {AxiosPromise}
 */
export function roleList() {
  return request({
    url: '/admin/role/list/1',
    method: 'get'
  })
}
